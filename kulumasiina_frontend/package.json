	},
	"devDependencies": {
		"@material/elevation": "^13.0.0",
		"@smui/button": "^6.1.0",
		"@smui/card": "^6.1.0",
		"@smui/checkbox": "^6.1.0",
		"@smui/common": "^6.1.0",
		"@smui/data-table": "^6.1.0",
		"@smui/dialog": "^6.1.0",
		"@smui/fab": "^6.1.0",
		"@smui/form-field": "^6.1.0",
		"@smui/icon-button": "^6.1.0",
		"@smui/image-list": "^6.1.0",
		"@smui/layout-grid": "^6.1.0",
		"@smui/linear-progress": "^6.1.0",
		"@smui/textfield": "^6.1.0",
